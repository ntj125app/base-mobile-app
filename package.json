{
    "name": "base-mobile-app",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "rm -rf dist && vite",
        "serve": "rm -rf dist && vite preview",
        "build": "rm -rf dist && vite build",
        "browser": "echo \"const supportedBrowsers = $(browserslist-useragent-regexp --allowHigherVersions);\n\nexport { supportedBrowsers };\" > src/browser.ts",
        "presstyle:fix": "npm run prettier:fix && npm run stylelint:fix && npm run eslint:fix",
        "eslint": "eslint --ext .js,.cjs,.ts,.vue src",
        "eslint:fix": "eslint --ext .js,.cjs,.ts,.vue src --fix",
        "stylelint": "stylelint src/**/*.{css,scss,vue}",
        "stylelint:fix": "stylelint src/**/*.{css,scss,vue} --fix",
        "prettier": "prettier --check src/**/*.{css,scss,vue,js,cjs,ts,json,md} tests/**/*.{css,scss,vue,js,cjs,ts,json,md} *.{css,scss,vue,js,cjs,ts,json,md}",
        "prettier:fix": "prettier --write src/**/*.{css,scss,vue,js,cjs,ts,json,md} tests/**/*.{css,scss,vue,js,cjs,ts,json,md} *.{css,scss,vue,js,cjs,ts,json,md}",
        "mobile:icon": "npx @capacitor/assets generate"
    },
    "description": "An Ionic project",
    "dependencies": {
        "@capacitor/android": "^5.5.1",
        "@capacitor/app": "^5.0.6",
        "@capacitor/core": "^5.5.1",
        "@capacitor/device": "^5.0.6",
        "@capacitor/haptics": "^5.0.6",
        "@capacitor/ios": "^5.5.1",
        "@capacitor/keyboard": "^5.0.6",
        "@capacitor/push-notifications": "^5.1.0",
        "@capacitor/splash-screen": "^5.0.6",
        "@capacitor/status-bar": "^5.0.6",
        "@capacitor/toast": "^5.0.6",
        "@ionic/pwa-elements": "^3.2.2",
        "@ionic/vue": "^7.5.6",
        "@ionic/vue-router": "^7.5.6",
        "@vueuse/core": "^10.6.1",
        "axios": "^1.6.2",
        "core-js": "^3.33.3",
        "dotenv": "^16.3.1",
        "ionicons": "^7.2.1",
        "laravel-echo": "^1.15.3",
        "lodash": "^4.17.21",
        "pinia": "^2.1.7",
        "primeicons": "^6.0.1",
        "primevue": "^3.41.0",
        "pusher-js": "^8.3.0",
        "vue": "^3.3.8",
        "vue-router": "^4.2.5"
    },
    "devDependencies": {
        "@capacitor/cli": "^5.5.1",
        "@pinia/testing": "^0.1.3",
        "@types/chai": "^4.3.11",
        "@types/jest": "^29.5.10",
        "@types/mocha": "^10.0.6",
        "@types/node": "^20.9.4",
        "@typescript-eslint/eslint-plugin": "^6.12.0",
        "@typescript-eslint/parser": "^6.12.0",
        "@vitejs/plugin-vue": "^4.5.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/test-utils": "^2.4.2",
        "autoprefixer": "^10.4.16",
        "browserslist-useragent-regexp": "^4.1.0",
        "cypress": "^13.6.1",
        "cypress-vite": "^1.5.0",
        "daisyui": "^4.4.2",
        "eslint": "^8.54.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-vue": "^9.18.1",
        "postcss": "^8.4.31",
        "postcss-convert-values": "^6.0.0",
        "prettier": "^3.1.0",
        "sass": "^1.69.5",
        "stylelint": "^15.11.0",
        "stylelint-config-recommended-vue": "^1.5.0",
        "stylelint-config-standard": "^34.0.0",
        "stylelint-config-standard-scss": "^11.1.0",
        "tailwindcss": "^3.3.5",
        "vite": "^5.0.2",
        "vite-plugin-static-copy": "^0.17.1"
    }
}

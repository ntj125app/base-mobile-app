        "@capacitor/ios": "^5.0.1",
        "@capacitor/keyboard": "^5.0.1",
        "@capacitor/push-notifications": "^5.0.1",
        "@capacitor/splash-screen": "^5.0.1",
        "@capacitor/status-bar": "^5.0.1",
        "@capacitor/toast": "^5.0.1",
        "@fortawesome/fontawesome-free": "^6.4.0",
        "@ionic/pwa-elements": "^3.1.1",
        "@ionic/vue": "^7.0.5",
        "@ionic/vue-router": "^7.0.5",
        "@vueuse/core": "^10.1.2",
        "axios": "^1.4.0",
        "core-js": "^3.30.2",
        "dotenv": "^16.0.3",
        "firebase": "^9.21.0",
        "ionicons": "^7.1.0",
        "laravel-echo": "^1.15.1",
        "pinia": "^2.0.35",
        "primeicons": "^6.0.1",
        "primevue": "^3.28.0",
        "pusher-js": "^8.0.2",
        "vue": "^3.2.47",
        "vue-router": "^4.1.6"
